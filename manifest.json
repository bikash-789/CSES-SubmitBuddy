{
    "manifest_version": 3,
    "name": "Code Auto-Submitter",
    "version": "1.0",
    "description": "Submit code to CSES after creating the file from pasted code.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon32.png",
      "128": "icon128.png"
    }
  }
  